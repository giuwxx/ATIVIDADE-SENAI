#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main() {
	
setlocale(LC_ALL,"portuguese");

int vetor[10],cubo[10],quadrado[10];

for(int i=0;i<10;i++){
    printf("Digite o %d° nÚmero: ",i+1);
    scanf("%d", &vetor[i]);
    quadrado[i]= vetor[i]*vetor[i];
    cubo[i]=vetor[i]*vetor[i]*vetor[i];
    printf("%d\n%d\n%d\n",vetor[i],quadrado[i],cubo[i]);
    }
}
